oute\n            path=\"/profile/:id\"\n            render={() => {\n              return <Profile loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route path=\"/:id\" component={Details} />\n        </Switch>\n        <Footer\n          loggedInUser={this.state.loggedInUser}\n          setCurrentUser={this.setCurrentUser}\n        />\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Signup.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Homepage.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Navbar.js",["78"],"import React from \"react\";\nimport { Link, NavLink } from \"react-router-dom\";\nimport AuthService from \"../utils/auth\";\nimport \"../App.css\";\nimport \"./Navbar.css\";\nimport MyModal from \"./Modal\";\n\nclass Navbar extends React.Component {\n  logoutUser = () => {\n    const authService = new AuthService();\n    authService.logout().then(() => {\n      this.props.setCurrentUser(null);\n      localStorage.removeItem(\"loggedInUser\");\n    });\n  };\n  render() {\n    if (this.props.loggedInUser) {\n      return (\n        <nav\n          className=\"navbar sticky-top navbar-light\"\n          id=\"nav-id2\"\n          style={{ backgroundColor: \"#e3f2fd\" }}\n        >\n          <div className=\"nav-bar-logo\">My First School</div>\n          <div className=\"nav-bar-links\">\n            <NavLink activeStyle={{ color: \"#5782ad\" }} exact to=\"/\">\n              Home\n            </NavLink>\n            <NavLink activeStyle={{ color: \"#5782ad\" }} exact to=\"/schools\">\n              All Schools\n            </NavLink>\n            <NavLink activeStyle={{ color: \"#5782ad\" }} exact to=\"/nurseries\">\n              Nurseries\n            </NavLink>\n            <NavLink\n              activeStyle={{ color: \"#5782ad\" }}\n              exact\n              to=\"/ki