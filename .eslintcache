/components/Nurseries\";\nimport Profile from \"./components/Profile\";\nimport Details from \"./components/Details\";\nimport \"./components/Navbar.css\";\nimport Footer from \"./components/Footer\";\n\nclass App extends React.Component {\n  state = {\n    loggedInUser: null,\n  };\n  setCurrentUser = (user) => {\n    this.setState({\n      loggedInUser: user,\n    });\n  };\n  componentDidMount() {\n    if (this.state.loggedInUser === null) {\n      const authService = new AuthService();\n      //check if the user session is still active on the server\n      //and set the user with current user\n      authService.loggedin().then((response) => {\n        if (response.data._id) {\n          this.setCurrentUser(response.data);\n          localStorage.setItem(\"loggedInUser\", response.data._id);\n        } else {\n          localStorage.removeItem(\"loggedInUser\");\n        }\n      });\n    }\n  }\n  render() {\n    return (\n      <div className=\"App\">\n        <ToastContainer />\n        <Navbar\n          loggedInUser={this.state.loggedInUser}\n          setCurrentUser={this.setCurrentUser}\n        />\n        <Switch>\n          <Route exact path=\"/\" component={Homepage} />\n          <Route\n            path=\"/signup\"\n            render={() => {\n              return <Signup setCurrentUser={this.setCurrentUser} />;\n            }}\n          />\n          <Route\n            path=\"/kindergartens\"\n            render={() => {\n              return <Kindergartens loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route\n            path=\"/nurseries\"\n            render={() => {\n              return <Nurseries loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route\n            path=\"/login\"\n            render={() => {\n              return <Login setCurrentUser={this.setCurrentUser} />;\n            }}\n          />\n          <Route\n            path=\"/schools\"\n            render={() => {\n              return <AllSchools loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route\n            path=\"/login-google\"\n            render={() => {\n              window.location.href = `${process.env.REACT_APP_PROJECTS_API}/api/auth/google`;\n            }}\n          />\n          <Route\n            path=\"/profile/:id\"\n            render={() => {\n              return <Profile loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route path=\"/:id\" component={Details} />\n        </Switch>\n        <Footer\n          loggedInUser={this.state.loggedInUser}\n          setCurrentUser={this.setCurrentUser}\n        />\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Navbar.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Login.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Homepage.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Kindergartens.js",["79"],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Nurseries.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Profile.js",["80","81","82"],"import React from \"react\";\nimport NurseriesDB from \"../utils/api\";\nimport { withRouter } from \"react-router-dom\";\nimport Kindergartens from \"./Kindergartens\";\nimport KindergartensApi from \"../utils/api-cml\";\nimport AuthService from \"../utils/auth\";\nimport { Link } from \"react-router-dom\";\n\nclass Profile extends React.Component {\n  state = {\n    favorites: [],\n  };\n\n  getFavorites2() {\n    const id = this.props.match.params.id;\n    // const name = this.props.loggedInUser.favorites;\n    const nurseries = new NurseriesDB();\n    nurseries.getFavorites(id).then((response) => {\n      console.log(response, \"response\");\n      this.setState({\n        favorites: response.data,\n      });\n    });\n  }\n\n  componentDidMount() {\n    this.getFavorites2();\n  }\n\n  handleDeleteFavorite = (id) => {\n    let userId = this.props.match.params.id;\n    const nurseries = new NurseriesDB();\n    console.log(id);\n    nurseries.deleteFavorite(id, userId).then(() => {\n      this.getFavorites2();\n      // this.props.history.push(`/profile/${id}`);\n      console.log({ message: \"the school was deleted\" });\n    });\n  };\n\n  render() {\n    console.log(this.state, \"profile state\");\n    return (\n      <div className=\"container\" style={{ minHeight: \"380px\" }}>\n        {/* <h2>Your favorites is empty</h2> */}\n        <div className=\"card-deck d-flex flex-wrap\">\n          {this.state.favorites.map((school, index) => {\n            return (\n              <div\n                className=\"card col-sm-4\"\n                key={index}\n                style={{ minWidth: \"30vh\" }}\n              >\n                <img\n                  src={school.photo}\n                  className=\"card-img-top\"\n                  id=\"card-img-top-profile\"\n                  alt=\"nurseries\"\n                />\n                <div className=\"card-body\">\n                  <Link\n                    to={{\n                      pathname:\n                        school.schoolType === \"kindergarten\"\n                          ? `/${school.GlobalID}`\n                          : `/${school._id}`,\n                      state: school,\n                    }}\n                  >\n                    {\" \"}\n                    <h5 className=\"card-title\">{school.name}</h5>{\" \"}\n                  </Link>\n                  <p className=\"card-text\">type: {school.schoolType}</p>\n                  <button\n                    id=\"delete-btn\"\n                    className=\"btn btn-primary\"\n                    onClick={() => this.handleDeleteFavorite(school._id)}\n                  >\n                    Delete Favorite\n                  </button>\n                </div>\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(Profile);\n","/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/AllSchools.js",["83"],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/utils/auth.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Footer.js",["84","85"],"import React from \"react\";\nimport { Link, NavLink } from \"react-router-dom\";\nimport \"./Footer.css\";\n\nclass Footer extends React.Component {\n  render() {\n    return (\n      <div className=\"bottom-section\">\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"col-sm\">\n              <h5 className=\"bottom-h5\">\n                <div className=\"nav-bar-logo-foot\">My First School</div>\n              </h5>\n              <a href=\"/\" className=\"bottom-p\">\n                About\n              </a>\n              <hr />\n              <a href=\"/\" className=\"bottom-p\">\n                Blog\n              </a>\n              <hr />\n              <a href=\"/\" className=\"bottom-p\">\n                Contact us\n              </a>\n              <hr />\n            </div>\n            <div className=\"col-sm\">\n              <h5 className=\"bottom-h5\">FOLLOW US</h5>\n              <a href=\"/\" className=\"bottom-p\">\n                <img\n                  id=\"fb-icon\"\n                  src=\"/Icons/fb-icon.png\"\n                  alt=\"facebook-icon\"\n                  style={{ width: \"30px\", height: \"23px\" }}\n                />\n              </a>\n              <hr />\n              <a href=\"/\" className=\"bottom-p\">\n                <img\n                  src=\"/Icons/ig vector.jpeg\"\n                  alt=\"IG-icon\"\n                  style={{\n     