n    return (\n      <div className=\"App\">\n        <ToastContainer />\n        <Navbar\n          loggedInUser={this.state.loggedInUser}\n          setCurrentUser={this.setCurrentUser}\n        />\n        <Switch>\n          <Route exact path=\"/\" component={Homepage} />\n          {/* <Route exact path=\"/schools\" component={ListProjects} /> */}\n          {/* <Route\n            exact\n            path=\"/projects/add\"\n            render={() => {\n              if (localStorage.getItem(\"loggedInUser\")) {\n                return <AddProject />;\n              } else {\n                return <Redirect to=\"/login\" />;\n              }\n            }}\n          />\n          <Route exact path=\"/projects/:id\" component={ProjectDetail} />\n          <Route path=\"/projects/:id/edit\" component={EditProject} /> */}\n          <Route path=\"/signup\" component={Signup} />\n          <Route path=\"/kindergartens\" component={Kindergartens} />\n          <Route path=\"/nurseries\" component={Nurseries} />\n          <Route\n            path=\"/login\"\n            render={() => {\n              return <Login setCurrentUser={this.setCurrentUser} />;\n            }}\n          />\n          <Route path=\"/schools\" component={AllSchools} />;\n          <Route\n            path=\"/login-google\"\n            render={() => {\n              window.location.href = `${process.env.REACT_APP_PROJECTS_API}/api/auth/google`;\n            }}\n          />\n        </Switch>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/reportWebVitals.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Signup.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Navbar.js",["66"],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Login.js",["67"],"import React from \"react\";\nimport AuthService from \"../utils/auth\";\nimport { NavLink } from \"react-router-dom\";\n\nimport { Link, withRouter } from \"react-router-dom\";\n\nclass Login extends React.Component {\n  state = {\n    username: \"\",\n    password: \"\",\n  };\n\n  handleChange = (event) => {\n    const { name, value } = event.target;\n    this.setState({ [name]: value });\n  };\n\n  handleFormSubmit = (event) => {\n    event.preventDefault();\n    const { username, password } = this.state;\n    const authService = new AuthService();\n    authService.login(username, password).then((response) => {\n      //lifting the state, function passed from App.js as props\n      this.props.setCurrentUser(response.data);\n      //save user id to browser local storage\n      localStorage.setItem(\"loggedInUser\", response.data._id);\n      this.props.history.push(\"/projects\");\n    });\n  };\n\n  render() {\n    return (\n      <div>\n        <div className=\"container-login\">\n          <div className=\"google-oauth\">\n            <button>\n              <img src=\"../../images/google.png\" className=\"google-icon\" />\n              <NavLink activeStyle={{ color: \"red\" }} exact to=\"/login-google\">\n                Login With Google\n              </NavLink>\n            </button>\n          </div>\n          <div className=\"login-signup\">\n            <form onSubmit={this.handleFormSubmit}>\n              {/* <label>Username:</label> */}\n              <input\n                type=\"text\"\n                name=\"username\"\n                placeholder=\"Your Username Here!\"\n                value={this.state.username}\n                onChange={this.handleChange}\n              />{\" \"}\n              <br />\n              {/* <label>Password:</label> */}\n              <input\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"Your Password Here!\"\n                value={this.state.password}\n                onChange={this.handleChange}\n              />\n              <br />\n              <button>Login</button>\n            </form>\n          </div>\n          <p>\n            Don't have account?\n            <Link to={\"/signup\"}> Signup</Link>\n          </p>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(Login);\n","/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Kindergartens.js",["68"],"import axios from \"axios\";\nimport React from \"react\";\nimport KindergartensApi from \"../utils/api-cml\";\n\nclass Kindergartens extends React.Component {\n  state = {\n    kindergartens: [],\n  };\n\n  componentDidMount() {\n    const kindergartens = new KindergartensApi();\n    kindergartens.getAllKindergartens().then((response) => {\n      return this.setState({\n        kindergartens: response.data.features,\n      });\n    });\n  }\n\n  //search method\n  // filterKindergarten = (id) => {\n  //   this.state.kindergartens.find((kindergarten) => {\n  //     console.log(id, kindergarten);\n  //     return id === kindergarten.attributes.GlobalID;\n  //   });\n  // };\n\n  render() {\n    return (\n      <div>\n        {this.state.kindergartens.map((kindergarten, index) => {\n          return (\n            <div className=\"card\" key={index} style={{ width: \"200px\" }}>\n              <img src=\"...\" className=\"card-img-top\" alt=\"...\" />\n              <div className=\"card-body\">\n                <h5 className=\"card-title\">\n                  {kindergarten.attributes.INF_NOME}\n                </h5>\n                <p className=\"card-text\">\n                  {kindergarten.attributes.INF_DESCRICAO}\n                </p>\n                {/* <a href=\"/\" className=\"btn btn-primary\">\n                  Go somewhere\n                </a> */}\n              </div>\n            </div>\n          );\n        })}\n      </div>\n    );\n  }\n}\n\nexport default Kindergartens;\n","/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/AllSchools.js",