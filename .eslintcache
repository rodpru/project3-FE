s\";\nimport \"./components/Navbar.css\";\nimport Footer from \"./components/Footer\";\n\nclass App extends React.Component {\n  state = {\n    loggedInUser: null,\n  };\n  setCurrentUser = (user) => {\n    this.setState({\n      loggedInUser: user,\n    });\n  };\n  componentDidMount() {\n    if (this.state.loggedInUser === null) {\n      const authService = new AuthService();\n      //check if the user session is still active on the server\n      //and set the user with current user\n      authService.loggedin().then((response) => {\n        if (response.data._id) {\n          this.setCurrentUser(response.data);\n          localStorage.setItem(\"loggedInUser\", response.data._id);\n        } else {\n          localStorage.removeItem(\"loggedInUser\");\n        }\n      });\n    }\n  }\n  render() {\n    return (\n      <div className=\"App\">\n        <ToastContainer />\n        <Navbar\n          loggedInUser={this.state.loggedInUser}\n          setCurrentUser={this.setCurrentUser}\n        />\n        <Switch>\n          <Route exact path=\"/\" component={Homepage} />\n          <Route path=\"/signup\" component={Signup} />\n          <Route\n            path=\"/kindergartens\"\n            render={() => {\n              return <Kindergartens loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route\n            path=\"/nurseries\"\n            render={() => {\n              return <Nurseries loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route\n            path=\"/login\"\n            render={() => {\n              return <Login setCurrentUser={this.setCurrentUser} />;\n            }}\n          />\n          <Route\n            path=\"/schools\"\n            render={() => {\n              return <AllSchools loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route\n            path=\"/login-google\"\n            render={() => {\n              window.location.href = `${process.env.REACT_APP_PROJECTS_API}/api/auth/google`;\n            }}\n          />\n          <Route\n            path=\"/profile/:id\"\n            render={() => {\n              return <Profile loggedInUser={this.state.loggedInUser} />;\n            }}\n          />\n          <Route path=\"/:id\" component={Details} />\n        </Switch>\n        <Footer\n          loggedInUser={this.state.loggedInUser}\n          setCurrentUser={this.setCurrentUser}\n        />\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Login.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Signup.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/AllSchools.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Nurseries.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Homepage.js",[],"/Users/migueltome/Desktop/ironhack/Project-3/project3-FE/src/components/Footer.js",["78","79"],"import React from \"react\";\nimport { Link, NavLink } from \"react-router-dom\";\nimport \"./Footer.css\";\n\nclass Footer extends React.Component {\n  render() {\n    return (\n      <div className=\"bottom-section\">\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"col-sm\">\n              <h5 className=\"bottom-h5\">\n                <div className=\"nav-bar-logo\">My First School</div>\n              </h5>\n              <a href=\"/\" className=\"bottom-p\">\n                About\n              </a>\n              <hr />\n              <a href=\"/\" className=\"bottom-p\">\n                Blog\n              </a>\n              <hr />\n              <a href=\"/\" className=\"bottom-p\">\n                Contact us\n              </a>\n              <hr />\n            </div>\n            <div className=\"col-sm\">\n              <h5 className=\"bot