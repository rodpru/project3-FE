 demo modal\n            </button> */}\n          </div>\n        </nav>\n      );\n    } else {\n      return (\n        <nav\n          className=\"navbar navbar-light \"\n          style={{ backgroundColor: \"#e3f2fd\" }}\n        >\n          <div className=\"nav-bar-logo\">My First School</div>\n          <div className=\"nav-bar-links\">\n            <NavLink activeStyle={{ color: \"red\" }} exact to=\"/\">\n              Home\n            </NavLink>\n            <NavLink activeStyle={{ color: \"red\" }} exact to=\"/schools\">\n              Schools\n            </NavLink>\n            <MyModal />\n\n            {/* <NavLink activeStyle={{ color: \"red\" }} exact to=\"/login\">\n              Login\n            </NavLink> */}\n            {/* <NavLink activeStyle={{ color: \"red\" }} exact to=\"/login-google\">\n              Login With Google\n            </NavLink> */}\n          </div>\n        </nav>\n      );\n    }\n  }\n}\nexport default Navbar;\n","/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/utils/api-cml.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/utils/api.js",[],"/Users/rp/Desktop/BootCamp/Project3/project-management-client/src/components/Modal.js",["83"],"import Modal from \"react-bootstrap/Modal\";\nimport Button from \"react-bootstrap/Button\";\nimport { NavLink, Link, withRouter } from \"react-router-dom\";\nimport AuthService from \"../utils/auth\";\n\nimport React from \"react\";\n\nclass MyModal extends React.Component {\n  state = {\n    show: false,\n    username: \"\",\n    password: \"\",\n  };\n\n  handleClose = () => {\n    this.setState({\n      show: false,\n    });\n  };\n\n  handleShow = () => {\n    this.setState({\n      show: true,\n    });\n  };\n\n  handleChange = (event) => {\n    const { name, value } = event.target;\n    this.setState({ [name]: value });\n  };\n\n  handleFormSubmit = (event) => {\n    event.preventDefault();\n    const { username, password } = this.state;\n